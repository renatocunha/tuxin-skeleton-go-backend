[Server]
IsProduction=false
ServerHost=<HOST>

[SslCert]
CertificateFile=cert/<FILE>.crt
PrivateKeyFile=cert/<FILE>.key
OtherCertificates[]=<CERT_FILE_1>,[<CERT_FILE_2>....]

[Auth0]
AccountDomain=<DOMAIN>

[NewRelic]
Licensekey=<LICENSE_KEY>
AppName=<APP_NAME>

[WebServer]
ClientPath=<CLIENT_PATH>
Routes=help,welcome
HttpsPort=10444
ReadTimeout=5
WriteTimeout=10
